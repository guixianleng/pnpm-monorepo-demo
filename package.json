{
  "name": "adv-ui",
  "version": "0.0.1",
  "description": "基于Element-plus二次封装基础组件",
  "private": false,
  "type": "module",
  "main": "lib/adv-ui.cjs.js",
  "module": "lib/adv-ui.esm.js",
  "types": "lib/types/components/index.d.ts",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "lib",
    "global.d.ts"
  ],
  "scripts": {
    "bootstrap": "pnpm install",
    "dev-demo": "pnpm run -C playground/demo dev",
    "lib": "rimraf lib && vite build",
    "build:no-cache": "pnpm store prune && pnpm lib",
    "build:analyze": "pnpm vite build --mode analyze",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "turbo run stub",
    "deploy": "deploy.sh",
    "pub": "npm publish --registry=https://registry.npmjs.org/",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "prettier": "prettier --write \"packages/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "eslint --cache --max-warnings 0 \"packages/**/*.{vue,ts,tsx}\" --fix",
    "commit:check": "pnpm lint && vue-tsc --noEmit",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && pnpm bootstrap",
    "watch": "nodemon --ignore lib/ --ignore node_modules/ --watch packages/ -C -e ts,tsx,scss,less --debug -x \"pnpm lib && yalc push\"",
    "docs:prettier": "prettier --write \"docs/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:preview": "pnpm run -C docs preview"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "co-utils-vue": "^2.5.1",
    "element-plus": "^2.8.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.2",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@adv/vite-config": "workspace:^",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "22.5.5",
    "@types/path-browserify": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/shared": "^3.5.6",
    "chalk": "^5.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.28.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "pinia-plugin-persistedstate": "^4.0.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.77.1",
    "sass-loader": "^14.2.1",
    "turbo": "^2.1.2",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.6",
    "vue": "^3.5.6",
    "vue-tsc": "^2.1.6",
    "vue-types": "^5.1.3",
    "yorkie-pnpm": "^2.0.1"
  },
  "keywords": [
    "element-plus",
    "vite",
    "vitepress",
    "vue3",
    "二次封装组件",
    "封装组件",
    "components"
  ],
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=9.0.2"
  },
  "gitHooks": {
    "pre-commit": "pnpm commit:check",
    "commit-msg": "node scripts/git-commit.js"
  }
}
{
  "name": "advint-ui-plus",
  "version": "1.4.12",
  "description": "基于Element-plus二次封装基础组件",
  "private": false,
  "type": "module",
  "main": "lib/t-ui-plus.umd.cjs",
  "module": "lib/t-ui-plus.umd.cjs",
  "types": "lib/index.d.ts",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "lib",
    "global.d.ts"
  ],
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "pnpm vite",
    "dev-demo": "pnpm run -C playground/demo dev",
    "lib-all": "rimraf lib && run-p type:check build:es build:browser",
    "lib": "rimraf lib && vite build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "build": "pnpm vite build",
    "build:es": "vite build",
    "build:browser": "vite -f iife build",
    "build:no-cache": "pnpm store prune && npm run build",
    "build:analyze": "pnpm vite build --mode analyze",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "turbo run stub",
    "deploy": "deploy.sh",
    "pub": "npm publish --registry=https://registry.npmjs.org/",
    "prettier": "prettier --write \"packages/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "eslint --cache --max-warnings 0 \"packages/**/*.{vue,ts,tsx}\" --fix",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap"
  },
  "dependencies": {
    "@adv/components": "workspace:*"
  },
  "devDependencies": {
    "@adv/vite-config": "workspace:*",
    "@element-plus/icons-vue": "^2.3.1",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "18.7.18",
    "@types/path-browserify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/shared": "^3.3.13",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.1.2",
    "chalk": "^5.2.0",
    "consola": "^3.1.0",
    "echarts": "^5.4.3",
    "element-plus": "^2.8.3",
    "escape-html": "^1.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.6.0",
    "jsencrypt": "3.0.0-rc.1",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^2.3.0",
    "prettier": "^2.7.1",
    "prismjs": "^1.29.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.8.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "turbo": "^2.1.2",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vite": "^5.4.6",
    "vue": "^3.5.6",
    "vue-tsc": "^2.1.6"
  },
  "keywords": [
    "element-plus",
    "vite",
    "vitepress",
    "vue3",
    "二次封装组件",
    "封装组件",
    "components"
  ],
  "packageManager": "pnpm@9.0.4",
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=9.0.2"
  }
}